    <script>
        arrBtn = devidePaginate(maxBtn);
        // phan trang
        function pagination(status){
            let count = 1;
            let urlParams = new URLSearchParams(window.location.search);
            let week = +urlParams.get('week')||+maxBtn;
            // let year = urlParams.get('year')||new Date().getFullYear();

